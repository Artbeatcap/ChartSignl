{
  "expo": {
    "name": "chartsignl",
    "slug": "chartsignl",
    "version": "1.0.0",
    "sdkVersion": "50.0.0",
    "scheme": "chartsignl",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "associatedDomains": ["applinks:chartsignl.com"]
    },
    "android": {
      "package": "com.chartsignl.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "chartsignl",
              "host": "auth",
              "pathPrefix": "/reset-password"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    }
  }
}
